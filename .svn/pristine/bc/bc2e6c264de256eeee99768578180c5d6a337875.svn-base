am-multiselect .dropdown-menu{
	padding-top: 3px;
    padding-left: 5px;
    padding-right: 5px;
    width: auto; 
    max-height:200px; 
    border: 1px solid #bed5cd; 
    overflow-y: scroll;  
    overflow-x: hidden; 
}

am-multiselect .dropdown-menu > li > a {
    padding: 3px 10px;
    cursor:pointer;
}

am-multiselect .dropdown-menu > li.selected {
    background-color: #ADD8E6;
}

am-multiselect .btn-default {
    color: #333;
    background-color: #fff; 
    border-color: #ccc;
}

<!-- Wrapper-->
<div id="wrapper" ng-controller="ToolConfigEventCtrl as cCtrl">

	<!-- Navigation -->
	<div ng-include="'views/common/navigation.html'"></div>

	<!-- Page wraper -->
	<!-- ng-class with current state name give you the ability to extended customization your view -->
	<div id="page-wrapper" class="gray-bg {{$state.current.name}}">

		<!-- Page wrapper -->
		<div ng-include="'views/common/topnavbar.html'"></div>

		<!-- Main view  -->
		<div class="row wrapper border-bottom white-bg page-heading">
			<div class="col-lg-10">

				<ol class="breadcrumb" style="padding-top: 5px;">
					<li><a href="index.html">Home</a></li>
					<li class="active"><strong>Tool Configuration Event</strong></li>
				</ol>
			</div>
			<div class="col-lg-2"></div>
		</div>

		<div class="wrapper wrapper-content animated fadeInRight">

			<div ng-include src="'views/widget/event_search.html'"></div>
			<div class="row">
				<div class="col-lg-6">
					<div class="ibox float-e-margins">
						<div class="ibox-title">
							<h5> Summary of Overridden Configurations</h5>
							<br />
						</div>
						<div class="ibox-content" style="min-height:400px;">
							<div class="animated fadeInDown" ng-show="!cCtrl.overriddenConfShow"
								no-data-graph></div>
							<canvas id="pie" class="chart chart-pie" ng-show="cCtrl.overriddenConfShow"
									chart-data="cCtrl.overriddenConfigData.count"
									chart-labels="cCtrl.overriddenConfigData.name" chart-legend="true"> 
									</canvas>
						</div>

					</div>
				</div>

			</div>
			
		</div>

		<!-- Footer -->
		<div ng-include="'views/common/footer.html'"></div>

	</div>
	<!-- End page wrapper-->

</div>
<!-- End wrapper-->
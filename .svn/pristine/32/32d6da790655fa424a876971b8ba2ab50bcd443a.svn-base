<div class="row">

	<div class="col-lg-11">
		<div class="ibox float-e-margins">
			<div class="ibox-title">
				<h5>Events</h5> <br/>
			</div>
			<div class="ibox-content">
				
				<table datatable="ng" ng-show="!eCtrl.loading" 
					class="table table-striped table-bordered table-hover dataTables-example">
					<thead>
						<tr>
							<th>Timestamp</th>
							<th>Event name</th>
							<th>Event Details</th>
						</tr>
					</thead>
					<tbody>
						<tr ng-repeat="e in allToolEvents" >
							<td class="sorting_1" nowrap>{{ e.metric_timestamp | date:'yyyy-MM-dd HH:mm:ss.sss'}}</td>
							<td>{{ e.metric_type }}</td>
							<td style="word-break: break-all;">{{ e.event_details }}</td>							
						</tr>
					</tbody>

				</table>
				<div class="spiner-example" ng-show="eCtrl.loading">
					<div class="sk-spinner sk-spinner-three-bounce">
						<div class="sk-bounce1"></div>
						<div class="sk-bounce2"></div>
						<div class="sk-bounce3"></div>
					</div>
				</div>
				
			</div>
		</div>
	</div>

</div>
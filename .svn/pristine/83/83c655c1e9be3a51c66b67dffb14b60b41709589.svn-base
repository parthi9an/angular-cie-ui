<div class="row">

	<div class="col-lg-12">
		<div class="ibox float-e-margins">
			<div class="ibox-title">
				<h5>Events</h5>
				<br />
			</div>
			<div class="ibox-content">

				<table datatable="ng" ng-show="!dbCtrl.eventTableLoading"
					dt-options="dbCtrl.dtOptions" dt-column-defs="dbCtrl.dtColumnDefs"
					dt-instance="dbCtrl.dtInstance"
					class="table table-striped table-bordered table-hover dataTables-example">
					<thead>
						<tr>
							<th>Date</th>
							<th>Name</th>
							<th>Details</th>
						</tr>
					</thead>
					<tbody>
						<tr ng-repeat="e in allToolEvents"> 
							<td nowrap>{{ e.metric_timestamp |
								date:'yyyy-MM-dd HH:mm:ss.sss'}}</td>
							<td>{{ e.metric_type }}</td>
							<td style="word-break: break-all;">
							   <span ng-repeat="(eKey, eValue) in e.event_details">
							     <span class=""> <b>{{eKey}}</b> </span> 
								 <span ng-repeat="(vcKey, vcValue) in eValue track by $index" ng-if="dbCtrl.isObject(eValue)"	style="color: blue;" >  
								   <span class=""><b>{{vcKey}}</b></span>
								   <span ng-repeat="(key, value) in vcValue track by $index" ng-if="dbCtrl.isObject(vcValue)">
								     <span class="">&nbsp;[&nbsp;<b>{{key}}</b></span>
								     <span> {{value}} &nbsp;]&nbsp;</span>
								   </span>  
								   <span ng-if="!dbCtrl.isObject(vcValue)"> {{vcValue}} &nbsp;|&nbsp;</span>  
								 </span> 
								 <span ng-if="!dbCtrl.isObject(eValue)"> {{eValue}} &nbsp;|&nbsp;</span>
							   </span>
							</td>

						</tr>
					</tbody>

				</table>
				<div class="spiner-example" ng-show="dbCtrl.eventTableLoading">
					<div class="sk-spinner sk-spinner-three-bounce">
						<div class="sk-bounce1"></div>
						<div class="sk-bounce2"></div>
						<div class="sk-bounce3"></div>
					</div>
				</div>
				
				<table id="entry-grid" datatable="" dt-options="dtTestOptions" dt-columns="dtTestColumns" class="table table-striped table-bordered table-hover dataTables-example"></table> 

			</div>
		</div>
	</div>

</div>
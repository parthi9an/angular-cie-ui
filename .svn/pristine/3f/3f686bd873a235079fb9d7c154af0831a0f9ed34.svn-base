<div class="inmodal">
	<div class="modal-header">
		<h4 class="modal-title">Filter Name</h4>
	</div>
	<div class="modal-body">
		<form role="form" class="form-inline">
			<div class="input-group">
				<input type="text" class="form-control"
					ng-model="sCtrl.filtername"
					placeholder="filtername{{sCtrl.totalFilterCount + 1}}">

			</div>

		</form>
	</div>
	<div class="modal-footer">
		<button type="button" class="btn btn-primary" ng-click="ok()">Save</button>
		<button type="button" class="btn btn-white" ng-click="cancel()">Cancel</button>
	</div>

</div>


<div class="row">

	<div class="col-lg-12">
		<div class="ibox float-e-margins">
			<div class="ibox-title">
				<h5>Events</h5>
				<br />
			</div>
			<div class="ibox-content">

				<table datatable="ng" ng-show="!dbCtrl.loading"
					class="table table-striped table-bordered table-hover dataTables-example">
					<thead>
						<tr>
							<th>Date</th>
							<th>Name</th>
							<th>Details</th>
						</tr>
					</thead>
					<tbody>
						<tr ng-repeat="e in allToolEvents">
							<td class="sorting_asc" nowrap>{{ e.metric_timestamp |
								date:'yyyy-MM-dd HH:mm:ss.sss'}}</td>
							<td>{{ e.metric_type }}</td>
							<td style="word-break: break-all;"><span
								ng-repeat="(key, value) in e.event_details"><span
									class=""><b>{{key}}</b></span><span>
										{{value}} &nbsp;|&nbsp;</span></span></td>
						</tr>
					</tbody>

				</table>
				<div class="spiner-example" ng-show="dbCtrl.loading">
					<div class="sk-spinner sk-spinner-three-bounce">
						<div class="sk-bounce1"></div>
						<div class="sk-bounce2"></div>
						<div class="sk-bounce3"></div>
					</div>
				</div>

			</div>
		</div>
	</div>

</div>
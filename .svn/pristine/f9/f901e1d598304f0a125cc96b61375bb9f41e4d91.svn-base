
<!-- Wrapper-->
<div id="wrapper" ng-controller="UserCtrl as uCtrl">

	<!-- Navigation -->
	<div ng-include="'views/common/navigation.html'"></div>

	<!-- Page wraper -->
	<!-- ng-class with current state name give you the ability to extended customization your view -->
	<div id="page-wrapper" class="gray-bg {{$state.current.name}}">

		<!-- Page wrapper -->
		<div ng-include="'views/common/topnavbar.html'"></div>

		<!-- Main view  -->
		<div class="row wrapper border-bottom white-bg page-heading">
			<div class="col-lg-10">

				<ol class="breadcrumb" style="padding-top: 5px;">
					<li><a href="index.html">Home</a></li>
					<li class="active"><strong>User Page</strong></li>
				</ol>
			</div>
			<div class="col-lg-2"></div>
		</div>

		<div class="wrapper wrapper-content animated fadeInRight">

			<div ng-include src="'views/widget/event_search.html'"></div>

			<div class="row">
				<div class="col-lg-6">
					<div class="ibox float-e-margins">
						<div class="ibox-title">
							<h5>Count of Sessions</h5>
							<br />
						</div>
						<div class="ibox-content" style="min-height:400px;">
							<canvas id="pie" class="chart chart-pie"
								chart-data="uCtrl.userSessionData.count"
								chart-labels="uCtrl.userSessionData.name" chart-legend="true"> 
									</canvas>
						</div>

					</div>
				</div>

				<div class="col-lg-6">
					<div class="ibox float-e-margins">
						<div class="ibox-title">
							<h5>Count of Users</h5>
							<br />
						</div>
						<div class="ibox-content" style="min-height:400px;">
							<canvas id="pie" class="chart chart-pie"
								chart-data="uCtrl.userData.count"
								chart-labels="uCtrl.userData.name" chart-legend="true"> 
									</canvas>
						</div>

					</div>
				</div>
				

			</div>
			<div class="row"></div>

		</div>

		<!-- Footer -->
		<div ng-include="'views/common/footer.html'"></div>

	</div>
	<!-- End page wrapper-->

</div>
<!-- End wrapper-->